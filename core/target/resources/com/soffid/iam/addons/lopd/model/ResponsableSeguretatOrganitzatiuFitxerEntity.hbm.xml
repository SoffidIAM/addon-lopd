<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-cascade='none' auto-import='false' >
	<class name = 'com.soffid.iam.addons.lopd.model.ResponsableSeguretatOrganitzatiuFitxerEntityImpl' dynamic-insert='false' dynamic-update='false' table='SCL_RSOFIT' >
		<id name='id' type='java.lang.Long' unsaved-value='null'>
			<column name='RES_ID'/>
			<generator class='es.caib.seycon.ng.model.identity.IdentityGenerator'/>
		</id>
		<many-to-one name='fitxer' class='com.soffid.iam.addons.lopd.model.FitxerEntityImpl' lazy='proxy' fetch='select' foreign-key='SCL_RSOFIT_RES_IDFIT'>
			<column name='RES_IDFIT' not-null='true'/>
		</many-to-one>
		<many-to-one name='responsableSeguretatOrganitzatiu' class='es.caib.seycon.ng.model.UsuariEntityImpl' lazy='proxy' fetch='select' foreign-key='SCL_RSOFIT_RES_IDUSU'>
			<column name='RES_IDUSU' not-null='true'/>
		</many-to-one>
	</class>
</hibernate-mapping>
